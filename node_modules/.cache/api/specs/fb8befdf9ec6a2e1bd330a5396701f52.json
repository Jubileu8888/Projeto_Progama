  "paths": {
    "/{instance_id}/api/v1/status": {
      "get": {
        "summary": "status",
        "description": "Retorna o status da conexão entre a instância e o WhatsApp.",
        "operationId": "status",
        "parameters": [
          {
            "name": "instance_id",
            "in": "path",
            "description": "Código da sua instância (exemplo: chatpro-fx5qbe2hah)",
            "schema": {
              "type": "string"
            },
            "required": true
          }
        ],
        "responses": {
          "200": {
            "description": "200",
            "content": {
              "application/json": {
                "examples": {
                  "Result": {
                    "value": "{\n  \"connected\": true,\n  \"power_save\": false,\n  \"name\": \"Nome da sua instância\",\n  \"info\": {\n    \"Battery\": 100,\n    \"Platform\": \"A plataforma na qual você escaneou o QR Code, podendo ser IOS ou Android\",\n    \"Connected\": true,\n    \"Pushname\": \"O seu nome no WhatsApp, definido por você no momento da criação da sua conta\",\n    \"Wid\": \"É o seu ID no WhatsApp\",\n    \"Lc\": \"BR\",\n    \"Phone\": {\n      \"Mcc\": \"000\",\n      \"Mnc\": \"000\",\n      \"OsVersion\": \"9\",\n      \"DeviceManufacturer\": \"Web Beta\",\n      \"DeviceModel\": \"octopus_cheets\",\n      \"OsBuildNumber\": \"R92-13982.82.0 release-keys\",\n      \"WaVersion\": \"2.21.21.19\"\n    },\n    \"Plugged\": true,\n    \"Tos\": 0,\n    \"Lg\": \"pt\",\n    \"Is24h\": true\n  }\n}"
                  }
                },
                "schema": {
                  "type": "object",
                  "properties": {
                    "connected": {
                      "type": "boolean",
                      "example": true,
                      "default": true
                    },
                    "power_save": {
                      "type": "boolean",
                      "example": false,
                      "default": true
                    },
                    "name": {
                      "type": "string",
                      "example": "Nome da sua instância"
                    },
                    "info": {
                      "type": "object",
                      "properties": {
                        "Battery": {
                          "type": "integer",
                          "example": 100,
                          "default": 0
                        },
                        "Platform": {
                          "type": "string",
                          "example": "A plataforma na qual você escaneou o QR Code, podendo ser IOS ou Android"
                        },
                        "Connected": {
                          "type": "boolean",
                          "example": true,
                          "default": true
                        },
                        "Pushname": {
                          "type": "string",
                          "example": "O seu nome no WhatsApp, definido por você no momento da criação da sua conta"
                        },
                        "Wid": {
                          "type": "string",
                          "example": "É o seu ID no WhatsApp"
                        },
                        "Lc": {
                          "type": "string",
                          "example": "BR"
                        },
                        "Phone": {
                          "type": "object",
                          "properties": {
                            "Mcc": {
                              "type": "string",
                              "example": "000"
                            },
                            "Mnc": {
                              "type": "string",
                              "example": "000"
                            },
                            "OsVersion": {
                              "type": "string",
                              "example": "9"
                            },
                            "DeviceManufacturer": {
                              "type": "string",
                              "example": "Web Beta"
                            },
                            "DeviceModel": {
                              "type": "string",
                              "example": "octopus_cheets"
                            },
                            "OsBuildNumber": {
                              "type": "string",
                              "example": "R92-13982.82.0 release-keys"
                            },
                            "WaVersion": {
                              "type": "string",
                              "example": "2.21.21.19"
                            }
                          }
                        },
                        "Plugged": {
                          "type": "boolean",
                          "example": true,
                          "default": true
                        },
                        "Tos": {
                          "type": "integer",
                          "example": 0,
                          "default": 0
                        },
                        "Lg": {
                          "type": "string",
                          "example": "pt"
                        },
                        "Is24h": {
                          "type": "boolean",
                          "example": true,
                          "default": true
                        }
                      }
                    }
                  }
                }
              }
            }
          },
          "401": {
            "description": "401",
            "content": {
              "application/json": {
                "examples": {
                  "Result": {
                    "value": "{\n  \"statusCode\": 401,\n  \"message\": \"Unauthorized\"\n}"
                  }
                },
                "schema": {
                  "type": "object",
                  "properties": {
                    "statusCode": {
                      "type": "integer",
                      "example": 401,
                      "default": 0
                    },
                    "message": {
                      "type": "string",
                      "example": "Unauthorized"
                    }
                  }
                }
              }
            }
          }
        },
        "deprecated": false
      }
    },
    "/{instance_id}/api/v1/generate_qrcode": {
      "get": {
        "summary": "generate_qrcode",
        "description": "Gera um QR Code para conectar-se ao chatPro \n\nO QR Code gerado está no formato `base64`\n\n_Obs: o QR Code também é exibido no <a href=\"https://api.chatpro.com.br/painel\" target=\"_blank\">Painel da API</a>._",
        "operationId": "generate_qrcode",
        "parameters": [
          {
            "name": "instance_id",
            "in": "path",
            "description": "Código da sua instância (ex: `chatpro-fx5qbe2hah`)",
            "schema": {
              "type": "string"
            },
            "required": true
          }
        ],
        "responses": {
          "200": {
            "description": "200",
            "content": {
              "application/json": {
                "examples": {
                  "Result": {
                    "value": "{\n\t\"qr\": \"QR code em base64\"\n  \"status\": true,\n  \"error\": \"\"\n}"
                  }
                }
              }
            }
          },
          "401": {
            "description": "401",
            "content": {
              "application/json": {
                "examples": {
                  "Result": {
                    "value": "{\n  \"statusCode\": 401,\n  \"message\": \"Unauthorized\"\n}"
                  }
                },
                "schema": {
                  "type": "object",
                  "properties": {
                    "statusCode": {
                      "type": "integer",
                      "example": 401,
                      "default": 0
                    },
                    "message": {
                      "type": "string",
                      "example": "Unauthorized"
                    }
                  }
                }
              }
            }
          }
        },
        "deprecated": false
      }
    },
